\n  },\n  methods: {\n    submit() {\n      firebase\n        .auth()\n        .createUserWithEmailAndPassword(this.form.email, this.form.password)\n        .then((data) => {\n          data.user\n            .updateProfile({\n              displayName: this.form.name,\n            })\n            .then(() => {});\n        })\n        .catch((err) => {\n          this.error = err.message;\n        });\n    },\n  },\n};\n</script>\n"]}]}