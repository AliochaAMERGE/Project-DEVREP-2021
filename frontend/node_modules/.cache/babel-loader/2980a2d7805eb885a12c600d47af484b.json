AA,MAAM,EAAE,aAAT;AAAwB,QAAA,SAAS,EAAE;AAAnC,OAAb;AACA,UAAM,GAAE,GAAI,KAAK,QAAL,GAAgB,MAA5B,CAHQ,CAId;;AACM,MAAA,KAAK,CAAC,GAAN,CAAU,GAAE,GAAI,IAAhB,EAAsB,IAAtB,CAA2B,UAAA,OAAM,EAAK;AACpC,QAAA,KAAI,CAAC,eAAL,CAAqB,OAAO,CAAC,EAA7B,EAAiC,OAAO,CAAC,OAAzC,EAAkD,OAAO,CAAC,QAA1D;AACD,OAFD;AAGD,KAfO;AAiBR,IAAA,QAjBQ,sBAiBG;AAAA;;AACT,UAAM,aAAY,GAAI,GAAtB;AACA,UAAM,IAAG,GAAI;AAAC,QAAA,MAAM,EAAE;AAAT,OAAb;AACA,UAAM,GAAE,GAAI,KAAK,QAAL,GAAgB,WAA5B;AACA,WAAK,GAAL,CAAS,GAAT,EAAc,IAAd,EAAoB,UAAC,OAAD;AAAA,eAChB,MAAI,CAAC,eAAL,CAAqB,OAAO,CAAC,EAA7B,EAAiC,OAAO,CAAC,OAAzC,EAAkD,OAAO,CAAC,QAA1D,CADgB;AAAA,OAApB;AAGD,KAxBO;AA0BR,IAAA,UA1BQ,wBA0BK;AAAA;;AACX,UAAM,GAAE,GAAI,KAAK,QAAjB;AACA,WAAK,GAAL,CAAS,GAAT,EAAc,UAAC,OAAD;AAAA,eACV,MAAI,CAAC,eAAL,CAAqB,OAAO,CAAC,EAA7B,EAAiC,OAAO,CAAC,OAAzC,EAAkD,OAAO,CAAC,QAA1D,CADU;AAAA,OAAd;AAGD,KA/BO;AAiCR,IAAA,eAjCQ,2BAiCQ,SAjCR,EAiCmB,cAjCnB,EAiCmC,eAjCnC,EAiCoD;AAC1D,UAAM,YAAW,GAAI,KAAK,KAAL,CAAW,eAAX,CAArB;AACA,MAAA,QAAQ,CAAC,cAAT,CAAwB,WAAxB,EAAqC,WAArC,GAAmD,SAAnD;AACA,MAAA,QAAQ,CAAC,cAAT,CAAwB,gBAAxB,EAA0C,WAA1C,GAAwD,cAAxD;AACA,MAAA,QAAQ,CAAC,cAAT,CAAwB,iBAAxB,EAA2C,WAA3C,GAAyD,YAAzD;AACD,KAtCO;AAwCR,IAAA,iBAxCQ,6BAwCU,QAxCV,EAwCoB;AAC1B,WAAK,cAAL,GAAsB,QAAQ,CAAC,OAA/B;AACF;AA1CQ;AAxBG,CAAf","sourcesContent":["<template>\n  <head>\n    <meta charset=\"utf-8\" />\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\" />\n    <title>Bank Web Application</title>\n    <link rel=\"apple-touch-icon\" href=\"./../static/apple-touch-icon.png\" />\n    <!-- Place favicon.ico in the root directory -->\n    <!--<link href=\"bulma.min.css\" /> -->\n    <link rel=\"stylesheet\" href=\"https://cdn.jsdelivr.net/npm/bulma@0.9.0/css/bulma.min.css\" />\n    <link rel=\"stylesheet\" href=\"./../static/styles/main.css\" />\n\n    <!-- Choose the icon between :\n        - \"./styles/banks/banque_postale.css\"\n        - \"./styles/banks/cic.css\"\n        - \"./styles/banks/societe_generale.css\"\n\n        ./styles/banks/banque_postale.css ./styles/banks/cic.css ./styles/banks/societe_generale.css\n      -->\n    <link rel=\"stylesheet\" href=\"./../static/styles/banks/banque_postale.css\" />\n  </head>\n\n\n  <body class=\"container\" style=\"margin-top: 5%\">\n  <!-- Columns for login functions -->\n  <div class=\"columns\">\n    <!-- Buttons -->\n    <div class=\"column\">\n      <span id=\"login-buttons-container\">\n        <button class=\"button is-info is-outlined\" id=\"google-login-button\" style=\"display: contents\">\n          <span class=\"icon\">\n            <i class=\"fab fa-google\"></i>\n          </span>\n          <span>Google login</span>\n        </button>\n        <button class=\"button is-link is-outlined\" id=\"microsoft-login-button\">\n          <span class=\"icon\">\n            <i class=\"fab fa-microsoft\"></i>\n          </span>\n          <span>Microsoft login</span>\n        </button>\n        <button class=\"button is-dark is-outlined\" id=\"github-login-button\">\n          <span class=\"icon\">\n            <i class=\"fab fa-github\"></i>\n          </span>\n          <span>Github login</span>\n        </button>\n      </span>\n      <span id=\"logout-button-container\">\n        <button class=\"button is-danger is-outlined\" id=\"logout-button\">\n          <span class=\"icon\">\n            <i class=\"fas fa-sign-out-alt\"></i>\n          </span>\n          <span>Logout</span>\n        </button>\n      </span>\n    </div>\n    <!-- Column that contains log states tags -->\n    <div class=\"column\" style=\"text-align: right\">\n      <span id=\"logged-tag\" class=\"tag is-success is-medium\">Connected</span>\n      <span id=\"not-logged-tag\" class=\"tag is-warning is-medium\">Not connected</span>\n    </div>\n  </div>\n\n  <!-- The name and description -->\n  <section class=\"section\">\n    <div class=\"container\">\n      <h1 class=\"title primary-color-text\" id=\"bankNameContainer\"></h1>\n      <h2 class=\"subtitle secondary-color-text\" id=\"bankDescriptionContainer\"></h2>\n    </div>\n  </section>\n\n  <div class=\"columns is-mobile\">\n    <!-- Account displaying -->\n    <div class=\"column is-4 is-offset-2\">\n      <div class=\"box\">\n        <article class=\"media\">\n          <div class=\"media-left\">\n            <figure class=\"image is-64x64\">\n              <!-- Choose the icon between :\n                  - \"./images/bank_icons/banque_postale.png\"\n                  - \"./images/bank_icons/cic.png\"\n                  - \"./images/bank_icons/societe_generale.png\"\n                  ./images/bank_icons/banque_postale.png ./images/bank_icons/cic.png ./images/bank_icons/societe_generale.png\n                -->\n              <img src=\"./../static/images/bank_icons/banque_postale.png\" alt=\"Image\" />\n            </figure>\n          </div>\n          <div class=\"media-content\">\n            <div class=\"content\">\n              <p>\n                <strong>Account nÂ°<span id=\"accountId\"></span></strong> <br />\n                <small>Current balance : <span id=\"accountBalance\"></span>\n                  <span id=\"accountCurrency\"></span></small>\n              </p>\n            </div>\n            <nav class=\"level is-mobile\">\n              <div class=\"level-left\">\n                <a class=\"level-item\" aria-label=\"info\">\n                  <span class=\"icon is-small primary-color-text\">\n                    <i class=\"fas fa-info-circle\" aria-hidden=\"true\"></i>\n                  </span>\n                </a>\n              </div>\n            </nav>\n          </div>\n        </article>\n      </div>\n    </div>\n    <div class=\"column is-4 is-offset-1\">\n      <iframe title=\"Converter\" src=\"https://xeconvert.com/widget1?from=usd&to=eur&lang=&theme=blue&font=12\"\n              width=\"100%\" height=\"100%\"></iframe>\n    </div>\n    <!-- Column that contains the Account box-->\n  </div>\n\n  <!-- The buttons -->\n  <div class=\"columns\">\n    <div class=\"column\" style=\"text-align: start\">\n      <button id=\"withdraw-button\" onclick=\"this.withdraw()\" class=\"button primary-color\">\n        <span class=\"icon\">\n          <i class=\"fas fa-arrow-down\"></i>\n        </span>\n        <span>Withdraw</span>\n      </button>\n      <!--<label class=\"checkbox\">\n          <input id=\"limit-checkbox\" type=\"checkbox\" onchange=\"limitationChanged(this)\">\n          With limitation\n        </label>-->\n    </div>\n    <div class=\"column\" style=\"text-align: end\">\n      <button id=\"deposit-button\" onclick=\"this.deposit()\" class=\"button secondary-color\">\n        <span class=\"icon\">\n          <i class=\"fas fa-arrow-up\"></i>\n        </span>\n        <span>Deposit</span>\n      </button>\n    </div>\n  </div>\n\n  <div class=\"columns is-mobile\">\n    <div class=\"column is-6\">\n      <div class=\"content secondary-color-text\">\n        Contact\n        <ul>\n          <li>\n            <i class=\"fas fa-phone\"></i>\n            <span id=\"phoneNumberContainer\"></span>\n          </li>\n          <li>\n            <i class=\"fas fa-envelope\"></i>\n            <span id=\"mailContainer\"></span>\n          </li>\n        </ul>\n      </div>\n    </div>\n\n    <div class=\"column is-offset-4 is-2\">\n      <div class=\"columns is-mobile\">\n        <div class=\"column is-half\">\n          <figure class=\"image is-64x64\">\n            <img src=\"src/static/images/mastercard.png\" alt=\"Image\" />\n          </figure>\n        </div>\n        <div class=\"column is-half\">\n          <figure class=\"image is-64x64\">\n            <img style=\"margin-top: 13px\" src=\"./../static/images/visa.png\" alt=\"Image\" />\n          </figure>\n        </div>\n      </div>\n    </div>\n  </div>\n\n  </body>\n\n</template>\n\n<script>\n// @ is an alias to /src\n\n\nimport {initWebPageWithData} from \"@/static/scripts/data\";\nimport axios from 'axios';\n\n\nexport default {\n  name: 'Home',\n  withLimitation: false,\n  components: {\n  },\n\n  data() {\n    const DOMAIN = \"https://localhost:8080\";\n    const BASE_URL = `${DOMAIN}/api/`;\n    const ICONS = {\n      Euro: '<i class=\"fas fa-euro-sign\"></i>',\n      Dollar: '<i class=\"fas fa-dollar-sign\"></i>',\n    };\n    return {\n      DOMAIN,\n      BASE_URL ,\n      ICONS\n    }\n  },\n\n  mounted() {\n    this.init()\n  },\n\n  methods : {\n\n    init() {\n      initWebPageWithData();\n      this.getAccount();\n    },\n\n    deposit() {\n      const defaultAmount = 200;\n      const args = {amount: defaultAmount, withLimit: true};\n      const url = this.BASE_URL + \"api/\";\n// TODO REFAIRE !!!\n      axios.get(url + args).then(account => {\n        this.fillAccountData(account.id, account.balance, account.currency)\n      })\n    },\n\n    withdraw() {\n      const defaultAmount = 200;\n      const args = {amount: defaultAmount};\n      const url = this.BASE_URL + \"Withdraw/\";\n      this.get(url, args, (account) =>\n          this.fillAccountData(account.id, account.balance, account.currency)\n      );\n    },\n\n    getAccount() {\n      const url = this.BASE_URL;\n      this.get(url, (account) =>\n          this.fillAccountData(account.id, account.balance, account.currency)\n      );\n    },\n\n    fillAccountData(accountId, accountBalance, accountCurrency) {\n      const currencyIcon = this.ICONS[accountCurrency];\n      document.getElementById('accountId').textContent = accountId;\n      document.getElementById(\"accountBalance\").textContent = accountBalance;\n      document.getElementById(\"accountCurrency\").htmlContent = currencyIcon;\n    },\n\n    limitationChanged(checkbox) {\n      this.withLimitation = checkbox.checked;\n    }\n\n  }\n}\n\n</script>\n"],"sourceRoot":""}]}